{
  "presets": [
    "@babel/preset-react",
    ["@babel/preset-env", {
      "targets": "last 2 versions, Firefox ESR, > 1%, ie >= 11",
      "useBuiltIns": "usage",
      "corejs": "3.6"
    }]
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties"
  ]
}
